{
  "name": "@aureooms/js-rational-field",
  "description": "Rational field for JavaScript",
  "version": "2.0.0",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-rational-field/issues"
  },
  "dependencies": {
    "@aureooms/js-error": "^4.0.1",
    "@aureooms/js-integer": "^9.0.2",
    "@aureooms/js-integer-big-endian": "^10.1.0",
    "@aureooms/js-number": "^3.1.0",
    "@aureooms/js-prime": "^4.0.0",
    "@aureooms/js-rational": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-async-generator-functions": "7.12.1",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.10",
    "@babel/register": "7.12.10",
    "ava": "3.14.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.0.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "xo": "0.36.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-rational-field",
  "keywords": [
    "big",
    "bigdec",
    "bigdecimal",
    "computation",
    "decimal",
    "exact",
    "field",
    "fraction",
    "rational"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-rational-field"
  },
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "esdoc": "esdoc",
    "lint": "xo",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "sideEffects": false,
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "off",
      "camelcase": "off"
    },
    "ignores": [
      "lib",
      "doc"
    ]
  }
}
