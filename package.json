{
  "name": "@aureooms/js-rational-field",
  "description": "Rational field for JavaScript",
  "version": "0.0.2",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-rational-field/issues"
  },
  "dependencies": {
    "@aureooms/js-error": "^4.0.0",
    "@aureooms/js-integer": "^6.0.0",
    "@aureooms/js-integer-big-endian": "^8.0.2",
    "@aureooms/js-number": "^3.1.0",
    "@aureooms/js-prime": "^4.0.0",
    "@aureooms/js-rational": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-async-generator-functions": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "ava": "3.8.1",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.3",
    "nyc": "15.0.1",
    "xo": "0.30.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-rational-field",
  "keywords": [
    "bigdec",
    "bigdecimal",
    "computation",
    "exact",
    "field",
    "fraction",
    "rational"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-rational-field"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "xo && ava"
  },
  "xo": {
    "prettier": true,
    "ignore": [
      "lib",
      "doc"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "off",
      "camelcase": "off"
    }
  }
}
